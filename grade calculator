m=int(input("Enter the maths marks"))
s=int(input("Enter the science marks"))
e=int(input("Enter the english marks"))
tot=m+s+e
avg=tot/3
print(f"total:{tot}\navg:{avg}")
if(avg>=90):
    print("Grade:A")
elif(avg>=80 and avg<90):
    print("Grade:B")
elif(avg>=70 and avg<80):
    print("Grade:C")
elif(avg>=60 and avg<70):
    print("Grade:D")
else:
    print("Fail")


